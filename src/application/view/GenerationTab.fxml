<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="application.controls.GenerationTabController">
	<children>
		<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="336.0"
			prefWidth="600.0">
			<children>
				<TextField fx:id="textField_UserName_Generation" layoutX="14.0"
					layoutY="31.0" prefHeight="25.0" prefWidth="480.0"
					text="Enter key owner name ..." />
				<Label layoutX="14.0" layoutY="14.0" text="User Name :" />
				<Button fx:id="button_Generate_Generation" layoutX="240.0"
					layoutY="276.0" mnemonicParsing="false" onAction="#button_Generate_Generation"
					prefHeight="50.0" prefWidth="330.0" text="Generate RSA Key" />
				<Label layoutX="14.0" layoutY="65.0" text="Password for private key :" />
				<PasswordField fx:id="passwordField_Password_Generation"
					layoutX="14.0" layoutY="82.0" prefHeight="25.0" prefWidth="480.0" />
			</children>
		</AnchorPane>
	</children>

</VBox>
