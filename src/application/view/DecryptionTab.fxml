<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="application.controls.DecryptionTabController">

	<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="336.0"
		prefWidth="600.0">
		<children>
			<TextField fx:id="textField_InputFile_Decryption" editable="false"
				layoutX="14.0" layoutY="31.0" prefWidth="480.0" text="Choose file to decrypt ..." />
			<TextField fx:id="textField_OutputFile_Decryption"
				editable="false" layoutX="14.0" layoutY="82.0" prefWidth="480.0"
				text="Choose path to save decrypted file ..." />
			<Button fx:id="button_LoadFile_Decryption" layoutX="507.0"
				layoutY="31.0" mnemonicParsing="false" onAction="#button_LoadFile_Decryption_Action"
				prefWidth="80.0" text="Load file" />
			<Button fx:id="button_SaveTo_Decryption" layoutX="507.0"
				layoutY="82.0" mnemonicParsing="false" onAction="#button_SaveTo_Decryption_Action"
				prefWidth="80.0" text="Save to" />
			<Label layoutX="14.0" layoutY="14.0" text="Input file :" />
			<Label layoutX="14.0" layoutY="65.0" text="Output file :" />
			<Label layoutX="285.0" layoutY="191.0" text="Password : " />
			<Button fx:id="button_Decrypt_Decryption" layoutX="240.0"
				layoutY="276.0" mnemonicParsing="false" onAction="#button_Decrypt_Dcryption_Action"
				prefHeight="50.0" prefWidth="330.0" text="Decrypt" />
			<ListView fx:id="listView_Recipents_Decryption" layoutX="14.0"
				layoutY="126.0" prefHeight="200.0" prefWidth="200.0" />
			<PasswordField fx:id="passwordField_Password_Decryption"
				layoutX="345.0" layoutY="187.0" onMouseClicked="#myszkoklik" />
		</children>
	</AnchorPane>

</VBox>
